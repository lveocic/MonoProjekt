@using static MonoProjekt.MVC.Controllers.VehicleMakeController

@model IEnumerable<VehicleMakeRestModel> 
@{
    ViewData["Title"] = "Index";
}

<h1>Index VehicleMake</h1>


<table class="table table-bordered table-striped" style="width:100%" >
	<thead>
		<tr>
			<th>
				VehicleMake Name
			</th>
			<th>
				Guid Id
			</th>
		</tr>
	</thead>
	<tbody>
		@foreach(var obj in Model)
		{
			<tr>
				<td width="50%">
					@obj.Name
				</td>
				<td width="30%">
					@obj.Id
				</td>
			</tr>
		}
	</tbody>
</table>

